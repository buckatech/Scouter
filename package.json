{
  "name": "scouter",
  "version": "1.0.0",
  "description": "A live power app for Alberta",
  "scripts": {
    "test": "mocha",
    "client": "cd client && yarn start",
    "server": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "dev:server": "cd client && yarn build && cd .. && yarn start",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buckatech/Scouter.git"
  },
  "keywords": [
    "power",
    "live",
    "alberta"
  ],
  "author": "Alex, Taylor, Kate",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/buckatech/Scouter/issues"
  },
  "homepage": "https://github.com/buckatech/Scouter#readme",
  "dependencies": {
    "chai-http": "^4.2.0",
    "cheerio": "^1.0.0-rc.2",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "mongodb": "^3.1.10",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.7",
    "puppeteer": "^1.11.0",
    "react-sunburst-d3-v4": "^0.1.6",
    "request": "^2.88.0",
    "request-promise": "^4.2.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "concurrently": "^4.1.0",
    "eslint": "5.3.0",
    "eslint-config-google": "^0.11.0",
    "mocha": "^5.2.0"
  }
}
